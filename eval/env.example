# Copy to `eval/.env` (or `.env` at repo root) and fill in values.
#
# Supabase credentials (required for live runs against stored outputs)
EVAL_SUPABASE_URL=__SUPABASE_PROJECT_URL__
EVAL_SUPABASE_SERVICE_KEY=__SUPABASE_SERVICE_ROLE_KEY__
#
# Where stored outputs live
EVAL_SUPABASE_TABLE=final_assets
EVAL_SUPABASE_SCHEMA=public
EVAL_SUPABASE_RUN_ID_COLUMN=run_id
#
# Optional: pin extraction keys if your stored output schema is stable
# EVAL_PREDICTION_ID_KEY=drug_name_code
# EVAL_PREDICTION_EVIDENCE_KEY=sources
# EVAL_PREDICTION_CYCLE_KEY=cycle


